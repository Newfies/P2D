{
    "manifest_version": 3,
    "name": "Patreon to Discord Notifier",
    "version": "1.2.0",
    "description": "Notify Discord via webhook when a new Patreon post is made.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "options_page": "options.html"
}